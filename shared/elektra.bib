@inbook{assmann2003invasive,
  address =       {Berlin, Heidelberg},
  author =        {A{\ss}mann, Uwe},
  booktitle =     {Invasive Software Composition},
  pages =         {107--145},
  publisher =     {Springer Berlin Heidelberg},
  title =         {Invasive Software Composition},
  year =          {2003},
  abstract =      {Invasive software composition (ISC) composes software
                   components by program transformation. Standard
                   composition treats components as immutable blackboxes
                   and plugs them together as they are. Invasive
                   software composition goes one step further and
                   transforms components when they are embedded in a
                   reuse context. In this way, components can be adapted
                   more appropriately to reuse requirements. The
                   components need not be adapted by hand, instead
                   invasive composition operators do all the work.},
  doi =           {10.1007/978-3-662-05082-8_4},
  isbn =          {978-3-662-05082-8},
  url =           {https://dx.doi.org/10.1007/978-3-662-05082-8_4},
